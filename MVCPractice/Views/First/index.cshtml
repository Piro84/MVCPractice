@using MVCPractice.Models;
@model  Employee

@{
    ViewBag.Title = "index";
}
<br />
<br />
<form action="SayHello" method="post">
    <input name="name" />
    <input name="age" type="number" />
    @for (int i = 0; i < Model.Qualifications.Length; i++)
    {
        <input type="text" name="Qualifications[i]" />
    }
    <input type="submit" name="submit" />

</form>
<p>@Model.name</p>
<p>@Model.age</p>
<p>@foreach (var qualification in Model.Qualifications)
{
    <li>@qualification</li>
}
    </p>