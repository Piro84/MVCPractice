
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <p>
            People
        </p>
        <div>
            @foreach (var i in Model)
            {
                <h1>@ViewBag.ItemList</h1>
                <b>ID : </b> @i.ID <br />
                <b>Category : </b> @i.FirstName <br />
                <b>Price : </b> @i.LastName <br />
                <hr />
            }
        </div>
    </div>
</body>
</html>
